package util;

public class Common {
	public static final String VIEW_PATH = "/WEB-INF/views/";
	
	public static class Users{
		public String userpath(String path) {
			return VIEW_PATH + "users/" + path + ".jsp";
		}
	}
	
	public static class Movies{
		public String userpath(String path) {
			return VIEW_PATH + "movies/" + path + ".jsp";
		}
	}
}
